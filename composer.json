{
    "name": "mehdiaroui/blogify",
    "description": "Add a pretty blog system to your website",
    "type": "library",
    "license": "mit",
    "authors": [
        {
            "name": "Mehdi Aroui",
            "email": "mehdipsy3@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php":">=7.1.3",
        "laravel/framework": "~5.8.0|^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Blogify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blogify\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blogify\\BlogifyServiceProvider"
            ]
        }
    }
}
